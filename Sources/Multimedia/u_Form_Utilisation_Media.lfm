    SQL.Strings = (
      'select p.mp_clef'
      ',p.mp_position'
      ',p.mp_table'
      ',p.mp_type_image'
      ',i.cle_fiche'
      ',i.sexe'
      ',i.nom'
      ',i.nom||coalesce('', ''||i.prenom,'''')||'
      '  iif(i.annee_naissance is null and i.annee_deces is null,'''','
      '  ''(''||coalesce(''°''||cast(annee_naissance as varchar(5)),'''')||'
      '  iif(annee_naissance is null or annee_deces is null,'''','' '')||'
      '  coalesce(''†''||cast(annee_deces as varchar(5)),'''')||'')'') as NOM_COMPLET'
      ',case when i.num_sosa>0  then 1 else 0 end as sosa'
      'from media_pointeurs p'
      'inner join individu i on i.cle_fiche=p.mp_cle_individu'
      'where p.mp_media=:cle_media'
      'order by i.nom,i.prenom,i.annee_naissance'
    )
