  object IBQRecherche: TIBQuery[7]
    Database = dm.ibd_BASE
    Transaction = dm.IBT_BASE
    BufferChunks = 1000
    CachedUpdates = False
    GeneratorField.ApplyOnEvent = gaeOnNewRecord
    SQL.Strings = (
      'select distinct s.Cle_fiche,s.Nom,s.Prenom,s.Sexe,s.num_sosa,s.surnom,s.annee_naissance,s.date_naissance,n.ev_ind_cp as cp_naissance,n.ev_ind_ville as lieu_naissance,n.ev_ind_subd as subd_naissance,n.ev_ind_pays as pays_naissance,s.annee_deces,s.date_deces,d.ev_ind_cp as cp_deces,d.ev_ind_ville as lieu_deces,d.ev_ind_subd as subd_deces,d.ev_ind_pays as pays_deces,s.type_lien from ('
      'select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''PAR'' as type_lien from individu i where i.cle_fiche=(select cle_pere from individu where cle_fiche=:indi) or i.cle_fiche=(select cle_mere from individu where cle_fiche=:indi)'
      ' union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''CPAR'' as type_lien from (select union_mari from t_union where union_femme=(select cle_mere from individu where cle_fiche=:indi and cle_mere is not null) and union_mari is not null and union_mari is distinct from (select cle_pere from individu where cle_fiche=:indi))as c inner join individu i on i.cle_fiche=c.union_mari union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''CPAR'' as type_lien from (select union_femme from t_union where union_mari=(select cle_pere from individu where cle_fiche=:indi and cle_pere is not null) and union_femme is not null and union_femme is distinct from (select cle_mere from individu where cle_fiche=:indi))as c inner join individu i on i.cle_fiche=c.union_femme union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''GP'' as type_lien from (select cle_fiche from proc_trouve_grands_parent(:indi)) as gp inner join individu i on i.cle_fiche=gp.cle_fiche union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''CGP'' as type_lien from (select cle_fiche from proc_trouve_grands_parent(:indi)where sexe=1) as gp inner join t_union u on u.union_mari=gp.cle_fiche and u.union_femme not in (select cle_fiche from proc_trouve_grands_parent(:indi)where sexe=2) inner join individu i on i.cle_fiche=u.union_femme union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''CGP'' as type_lien from (select cle_fiche from proc_trouve_grands_parent(:indi)where sexe=2) as gp inner join t_union u on u.union_femme=gp.cle_fiche and u.union_mari not in (select cle_fiche from proc_trouve_grands_parent(:indi)where sexe=1) inner join individu i on i.cle_fiche=u.union_mari union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''OT'' as type_lien from individu o inner join individu p on p.cle_fiche=o.cle_pere  inner join individu i on ((p.cle_pere is not null and i.cle_pere=p.cle_pere) or (p.cle_mere is not null and i.cle_mere=p.cle_mere)) and i.cle_fiche<>p.cle_fiche where o.cle_fiche=:indi and o.cle_pere is not null union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''OT'' as type_lien from individu o inner join individu p on p.cle_fiche=o.cle_mere  inner join individu i on ((p.cle_pere is not null and i.cle_pere=p.cle_pere) or (p.cle_mere is not null and i.cle_mere=p.cle_mere)) and i.cle_fiche<>p.cle_fiche where o.cle_fiche=:indi and o.cle_mere is not null union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''COT'' as type_lien from individu o inner join individu p on p.cle_fiche=o.cle_pere inner join individu t on ((p.cle_pere is not null and t.cle_pere=p.cle_pere) or (p.cle_mere is not null and t.cle_mere=p.cle_mere)) and t.cle_fiche<>p.cle_fiche inner join t_union u on t.cle_fiche in (u.union_mari,u.union_femme) inner join individu i on i.cle_fiche<>t.cle_fiche and i.cle_fiche in (u.union_mari,u.union_femme) where o.cle_fiche=:indi and o.cle_pere is not null union select i.Cle_fiche,i.Nom,i.Prenom,i.Sexe,i.num_sosa,i.annee_naissance,i.date_naissance,i.annee_deces,i.date_deces,i.surnom,''COT'' as type_lien from individu o inner join individu p on p.cle_fiche=o.cle_mere inner join individu t on ((p.cle_pere is not null and t.cle_pere=p.cle_pere) or (p.cle_mere is not null and t.cle_mere=p.cle_mere)) and t.cle_fiche<>p.cle_fiche inner join t_union u on t.cle_fiche in (u.union_mari,u.union_femme) inner join individu i on i.cle_fiche<>t.cle_fiche and i.cle_fiche in (u.union_ma