  object IBQEve: TIBQuery[3]
    Database = dm.ibd_BASE
    Transaction = dm.IBT_BASE
    BeforeClose = IBQEveBeforeScrollOrClose
    BeforeScroll = IBQEveBeforeScrollOrClose
    OnCalcFields = IBQEveCalcFields
    OnNewRecord = IBQEveNewRecord
    BufferChunks = 30
    CachedUpdates = False
    GeneratorField.ApplyOnEvent = gaeOnNewRecord
    SQL.Strings = (
      'select e.ev_ind_clef'
      ',e.ev_ind_kle_fiche'
      ',e.ev_ind_kle_dossier'
      ',e.ev_ind_type'
      ',e.ev_ind_date_writen'
      ',e.ev_ind_date_year'
      ',e.ev_ind_date'
      ',e.ev_ind_cp'
      ',e.ev_ind_ville'
      ',e.ev_ind_dept'
      ',e.ev_ind_pays'
      ',e.ev_ind_cause'
      ',e.ev_ind_source'
      ',e.ev_ind_comment'
      ',e.ev_ind_description'
      ',e.ev_ind_region'
      ',e.ev_ind_subd'
      ',e.ev_ind_acte'
      ',e.ev_ind_insee'
      ',e.ev_ind_heure'
      ',e.ev_ind_ordre'
      ',case e.ev_ind_type'
      '     when ''EVEN'' then coalesce(e.ev_ind_titre_event,r.ref_eve_lib_long)'
      '     else e.ev_ind_titre_event'
      '     end as ev_ind_titre_event'
      ',e.ev_ind_latitude'
      ',e.ev_ind_longitude'
      ',(select first(1) mp_media from media_pointeurs'
      '  where mp_pointe_sur=e.ev_ind_clef'
      '  and mp_table=''I'''
      '  and mp_type_image=''A'') as ev_ind_media'
      ',(select 1 from rdb$database'
      '  where exists (select 1'
      '       from sources_record s'
      '       left join media_pointeurs p on p.mp_table=''F'''
      '          and p.mp_type_image=''F'''
      '          and p.mp_pointe_sur=s.id'
      '       where s.type_table=''I'''
      '         and s.data_id=e.ev_ind_clef'
      '         and (s.auth>'''' or s.titl>'''' or s.abr>'''' or s.publ>'''' or s.texte>'''''
      '             or p.mp_media is not null))) as ev_ind_details'
      ',e.ev_ind_date_mois'
      ',e.ev_ind_date_year_fin'
      ',e.ev_ind_date_mois_fin'
      ',e.ev_ind_date_jour'
      ',e.ev_ind_date_jour_fin'
      ',e.ev_ind_type_token1'
      ',e.ev_ind_type_token2'
      ',e.ev_ind_datecode'
      ',e.ev_ind_datecode_tot'
      ',e.ev_ind_datecode_tard'
      ',e.ev_ind_calendrier1'
      ',e.ev_ind_calendrier2'
      ',r.REF_EVE_LIB_LONG'
      ',r.REF_EVE_CAT'
      'from  evenements_ind e'
      'inner join ref_evenements r on r.ref_eve_lib_court = e.ev_ind_type and r.ref_eve_langue=:langue'
      'where  e.ev_ind_kle_fiche = :cle_fiche and e.ev_ind_type<>''RESI'''
      'order by e.ev_ind_ordre nulls last'
      '  ,e.ev_ind_datecode nulls last'
      '  ,e.ev_ind_heure nulls last'
      '  ,r.ref_eve_ecran'
      '  ,e.ev_ind_clef'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'langue'
        ParamType = ptUnknown
      end    
      item
        DataType = ftUnknown
        Name = 'CLE_FICHE'
        ParamType = ptUnknown
      end>
    UpdateObject = IBUEve
    left = 120
    top = 160
    ParamData = <    
