  object IBQtv: TIBSQL[15]
    Database = dm.ibd_BASE
    ParamCheck = True
    SQL.Strings = (
      'SELECT c.CLE_FICHE'
      ',c.NOM||coalesce('', ''||c.PRENOM,'''') as nomprenom'
      ',c.SEXE'
      ',c.SOSA'
      ',iif(ne.ev_ind_date_writen is null and de.ev_ind_date_writen is '
      'null,null,iif(ne.ev_ind_date_writen is null,'
      '    ''(†''||de.ev_ind_date_writen||'')'',''(°''||ne.ev_ind_date_writen'
      '||coalesce('' †''||de.ev_ind_date_writen,'''')||'')'')) as naissdeces'
      ',c.union_clef'
      ',c.CLE_CONJOINT'
      ',c.NOM_CONJOINT'
      ',c.NOM_CONJOINT||coalesce('', ''||c.PRENOM_CONJOINT,'''') as nomprenom_CONJOINT'
      ',c.SEXE_CONJOINT'
      ',c.SOSA_CONJOINT'
      ',iif(nc.ev_ind_date_writen is null and dc.ev_ind_date_writen is '
      'null,null,iif(nc.ev_ind_date_writen is null,'
      '    ''(†''||dc.ev_ind_date_writen||'')'',''(°''||nc.ev_ind_date_writen'
      '||coalesce('' †''||dc.ev_ind_date_writen,'''')||'')'')) as naissdeces_CONJOINT'
      ',c.an_mar'
      ',ne.ev_ind_subd as subdnaissanceenfant'
      ',ne.ev_ind_date_writen as naissanceenfant'
      ',ne.ev_ind_ville as villenaissanceenfant'
      ',ne.ev_ind_dept as deptnaissanceenfant'
      ',ne.ev_ind_region as regionnaissanceenfant'
      ',ne.ev_ind_pays as paysnaissanceenfant'
      ',de.ev_ind_date_writen as decesenfant'
      ',de.ev_ind_subd as subddecesenfant'
      ',de.ev_ind_ville as villedecesenfant'
      ',de.ev_ind_dept as deptdecesenfant'
      ',de.ev_ind_region as regiondecesenfant'
      ',de.ev_ind_pays as paysdecesenfant'
      ',nc.ev_ind_date_writen as naissanceconjoint'
      ',nc.ev_ind_subd as subdnaissanceconjoint'
      ',nc.ev_ind_ville as villenaissanceconjoint'
      ',nc.ev_ind_dept as deptnaissanceconjoint'
      ',nc.ev_ind_region as regionnaissanceconjoint'
      ',nc.ev_ind_pays as paysnaissanceconjoint'
      ',dc.ev_ind_date_writen as decesconjoint'
      ',dc.ev_ind_subd as subddecesconjoint'
      ',dc.ev_ind_ville as villedecesconjoint'
      ',dc.ev_ind_dept as deptdecesconjoint'
      ',dc.ev_ind_region as regiondecesconjoint'
      ',dc.ev_ind_pays as paysdecesconjoint'
      ',c.cle_parents'
      ''
      'from (SELECT coalesce(e.CLE_FICHE,0) as CLE_FICHE'
      ',e.NOM'
      ',e.PRENOM'
      ',e.SEXE'
      ',e.cle_parents'
      ',iif(e.num_sosa>0,1,0) as SOSA'
      ',u.union_clef'
      ',coalesce(i.CLE_FICHE,0) as CLE_CONJOINT'
      ',coalesce(i.NOM,''?'') as NOM_CONJOINT'
      ',i.PRENOM as PRENOM_CONJOINT'
      ',-coalesce(i.SEXE,0) as SEXE_CONJOINT'
      ',iif(i.num_sosa>0,1,0) as SOSA_CONJOINT'
      ',e.date_naissance as date_naissance'
      ',e.date_deces as date_deces'
      ',i.date_naissance as date_naissance_CONJOINT'
      ',i.date_deces as date_deces_CONJOINT'
      ',(select first(1) ev_fam_date_year from evenements_fam'
      '  where ev_fam_kle_famille=u.union_clef and ev_fam_type=''MARR'' and ev_fam_date_year is not null'
      '  order by ev_fam_datecode) as an_mar'
      ',(select first(1) ev_fam_datecode from evenements_fam'
      '  where ev_fam_kle_famille=u.union_clef and ev_fam_date_year is not null'
      '  order by ev_fam_datecode) as date_prem_fam'
      ',(select first(1) n.ev_ind_datecode from individu e inner join evenements_ind n on n.ev_ind_kle_fiche=e.cle_fiche'
      '  where e.cle_pere is not distinct from u.union_mari and e.cle_mere is not distinct from u.union_femme and n.ev_ind_date_year is not null'
      '  order by n.ev_ind_datecode) as date_prem_enf'
      ',(select first(1) ev_ind_datecode from evenements_ind'
      '  where ev_ind_kle_fiche=i.cle_fiche and ev_ind_date_year is not null'
      '  order by ev_ind_datecode desc) as date_dern_eve'
      ',(select first(1) ev_ind_datecode*1460+coalesce(extract(hour from ev_ind_heure)*60+extract(minute from ev_ind_heure),0)'
      '  from evenements_ind'
      '  where ev_ind_kle_fiche=e.cle_fiche and ev_ind_date_year is not null'
      '  order by ev_ind_datecode,ev_ind_heure) as code_date_naissance'
      ''
      'FROM t_union u'
      'left join individu i on i.cle_fiche in (u.union_femme,u.union_mari) and i.cle_fiche<>:cle_fiche'
      'left join individu e on e.cle_pere is not distinct from u.union_mari and e.cle_mere is not distinct from u.union_femme'
      'Where u.union_mari=:cle_fiche or u.union_femme=:cle_fiche) as c'
      'left join evenements_ind nc on nc.ev_ind_type=''BIRT'' and nc.ev_ind_kle_fiche=c.cle_conjoint'
      'left join evenements_ind dc on dc.ev_ind_type=''DEAT'' and dc.ev_ind_kle_fiche=c.cle_conjoint'
      'left join evenements_ind ne on ne.ev_ind_type=''BIRT'' and ne.ev_ind_kle_fiche=c.cle_fiche'
      'left join evenements_ind de on de.ev_ind_type=''DEAT'' and de.ev_ind_kle_fiche=c.cle_fiche'
      ''
      'ORDER BY case'
      '  when (c.date_prem_enf is null) and (c.date_prem_fam is null) then c.date_dern_eve'
      '  when (c.date_prem_enf is null) then c.date_prem_fam'
      '  when (c.date_prem_fam is null) then c.date_prem_enf'
      '  when (c.date_prem_fam<=date_prem_enf) then c.date_prem_fam'
      '  else c.date_prem_enf'
      '  end'
      ',c.union_clef'
      ',c.cle_parents'
      ',c.code_date_naissance'
    )
    Transaction = dm.IBT_BASE
    left = 480
    top = 216
  end
