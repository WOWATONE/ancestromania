  object IBQmariageParents: TIBSQL[19]
    Database = dm.ibd_BASE
    ParamCheck = True
    SQL.Strings = (
      'select first(1)'
      'coalesce(p.nom,(select first(1) r.ref_eve_lib_long from evenements_ind e'
      'inner join ref_evenements r on r.ref_eve_lib_court=''IDSPE'' and r.ref_eve_langue=ds.ds_langue'
      'where e.ev_ind_kle_fiche=i.cle_fiche'
      'and e.ev_ind_type=''EVEN'''
      'and e.ev_ind_titre_event in (select ref_eve_lib_long from ref_evenements'
      'where ref_eve_lib_court in (''IDSPE'',''IDSPA'')'
      'and ref_eve_langue=ds.ds_langue))) as nom_pere'
      ',p.prenom as prenom_pere'
      ',p.date_naissance as pere_naissance'
      ',p.date_deces as pere_deces'
      ',p.num_sosa as pere_sosa'
      ',pn.ev_ind_subd as pere_subd_naissance'
      ',pn.ev_ind_ville as pere_ville_naissance'
      ',pn.ev_ind_dept as pere_dept_naissance'
      ',pn.ev_ind_region as pere_region_naissance'
      ',pn.ev_ind_pays as pere_pays_naissance'
      ',pd.ev_ind_subd as pere_subd_deces'
      ',pd.ev_ind_ville as pere_ville_deces'
      ',pd.ev_ind_dept as pere_dept_deces'
      ',pd.ev_ind_region as pere_region_deces'
      ',pd.ev_ind_pays as pere_pays_deces'
      ',coalesce(m.nom,(select first(1) r.ref_eve_lib_long from evenements_ind e'
      'inner join ref_evenements r on r.ref_eve_lib_court=''IDSME'' and r.ref_eve_langue=ds.ds_langue'
      'where e.ev_ind_kle_fiche=i.cle_fiche'
      'and e.ev_ind_type=''EVEN'''
      'and e.ev_ind_titre_event in(select ref_eve_lib_long from ref_evenements'
      'where ref_eve_lib_court in(''IDSME'',''IDSPA'')'
      'and ref_eve_langue=ds.ds_langue))) as nom_mere'
      ',m.prenom as prenom_mere'
      ',m.date_naissance as mere_naissance'
      ',m.date_deces as mere_deces'
      ',m.num_sosa as mere_sosa'
      ',mn.ev_ind_subd as mere_subd_naissance'
      ',mn.ev_ind_ville as mere_ville_naissance'
      ',mn.ev_ind_dept as mere_dept_naissance'
      ',mn.ev_ind_region as mere_region_naissance'
      ',mn.ev_ind_pays as mere_pays_naissance'
      ',md.ev_ind_subd as mere_subd_deces'
      ',md.ev_ind_ville as mere_ville_deces'
      ',md.ev_ind_dept as mere_dept_deces'
      ',md.ev_ind_region as mere_region_deces'
      ',md.ev_ind_pays as mere_pays_deces'
      ',''{X ''||f.ev_fam_date_writen||''}'' as date_union_parents'
      ',f.ev_fam_subd'
      ',f.ev_fam_ville'
      ',f.ev_fam_dept'
      ',f.ev_fam_region'
      ',f.ev_fam_pays'
      'from individu i'
      'inner join dossier ds on ds.cle_dossier=i.kle_dossier'
      'left join individu p on p.cle_fiche=i.cle_pere'
      'left join individu m on m.cle_fiche=i.cle_mere'
      'left join evenements_ind pn on pn.ev_ind_kle_fiche=p.cle_fiche and pn.ev_ind_type=''BIRT'''
      'left join evenements_ind pd on pd.ev_ind_kle_fiche=p.cle_fiche and pd.ev_ind_type=''DEAT'''
      'left join evenements_ind mn on mn.ev_ind_kle_fiche=m.cle_fiche and mn.ev_ind_type=''BIRT'''
      'left join evenements_ind md on md.ev_ind_kle_fiche=m.cle_fiche and md.ev_ind_type=''DEAT'''
      'left join t_union u on u.union_mari=i.cle_pere and u.union_femme=i.cle_mere'
      'left join evenements_fam f on f.ev_fam_kle_famille=u.union_clef and f.ev_fam_type=''MARR'''
      'where i.cle_fiche=:i_clef'
      'order by f.ev_fam_datecode'
    )
    Transaction = dm.IBT_BASE
    left = 232
    top = 320
  end
