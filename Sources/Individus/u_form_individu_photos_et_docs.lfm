  object IBMultimedia: TIBQuery[6]
    Database = dm.ibd_BASE
    Transaction = dm.IBT_BASE
    AfterOpen = IBMultimediaAfterOpen
    AfterScroll = IBMultimediaAfterScroll
    BeforeClose = IBMultimediaBeforeClose
    BeforeScroll = IBMultimediaBeforeScroll
    BufferChunks = 1000
    CachedUpdates = False
    GeneratorField.ApplyOnEvent = gaeOnNewRecord
    SQL.Strings = (
      'select 0 as ordre'
      ',m.multi_clef'
      ',m.multi_infos'
      ',m.multi_media'
      ',m.multi_memo'
      ',m.multi_nom'
      ',m.multi_reduite'
      ',m.multi_image_rtf'
      ',m.multi_path'
      ',p.MP_CLEF'
      ',p.mp_identite'
      ',p.mp_position'
      'FROM MEDIA_POINTEURS p '
      'inner join MULTIMEDIA m on p.MP_MEDIA=m.MULTI_CLEF'
      'WHERE p.mp_type_image=''I'' and p.MP_CLE_INDIVIDU=:indi'
      'union'
      'select e.ev_ind_datecode as ordre'
      ',m.multi_clef'
      ',m.multi_infos'
      ',m.multi_media'
      ',m.multi_memo'
      ',m.multi_nom'
      ',m.multi_reduite'
      ',m.multi_image_rtf'
      ',m.multi_path'
      ',p.MP_CLEF'
      ',p.mp_identite'
      ',p.mp_position'
      'FROM evenements_ind e '
      'inner join MEDIA_POINTEURS p on p.mp_type_image=''A'' and p.mp_table=''I'' and '
      'p.mp_pointe_sur=e.ev_ind_clef'
      'inner join MULTIMEDIA m on p.MP_MEDIA=m.MULTI_CLEF'
      'WHERE e.ev_ind_kle_fiche=:indi'
      'union'
      'select f.ev_fam_datecode as ordre'
      ',m.multi_clef'
      ',m.multi_infos'
      ',m.multi_media'
      ',m.multi_memo'
      ',m.multi_nom'
      ',m.multi_reduite'
      ',m.multi_image_rtf'
      ',m.multi_path'
      ',p.MP_CLEF'
      ',p.mp_identite'
      ',p.mp_position'
      'FROM t_union u '
      'inner join evenements_fam f on f.ev_fam_kle_famille=u.union_clef'
      'inner join MEDIA_POINTEURS p on p.mp_type_image=''A'' and p.mp_table=''F'' and '
      'p.mp_pointe_sur=f.ev_fam_clef'
      '  and p.mp_cle_individu=:indi'
      'inner join MULTIMEDIA m on p.MP_MEDIA=m.MULTI_CLEF'
      'WHERE u.union_mari=:indi or u.union_femme=:indi'
      'union'
      'select e.ev_ind_datecode as ordre'
      ',m.multi_clef'
      ',m.multi_infos'
      ',m.multi_media'
      ',m.multi_memo'
      ',m.multi_nom'
      ',m.multi_reduite'
      ',m.multi_image_rtf'
      ',m.multi_path'
      ',p.MP_CLEF'
      ',p.mp_identite'
      ',p.mp_position'
      'FROM evenements_ind e '
      'inner join sources_record s on s.type_table=''I'' and s.data_id=e.ev_ind_clef'
      'inner join MEDIA_POINTEURS p on p.mp_type_image=''F'' and p.mp_pointe_sur=s.id'
      'inner join MULTIMEDIA m on p.MP_MEDIA=m.MULTI_CLEF'
      'WHERE e.ev_ind_kle_fiche=:indi'
      'union'
      'select f.ev_fam_datecode as ordre'
      ',m.multi_clef'
      ',m.multi_infos'
      ',m.multi_media'
      ',m.multi_memo'
      ',m.multi_nom'
      ',m.multi_reduite'
      ',m.multi_image_rtf'
      ',m.multi_path'
      ',p.MP_CLEF'
      ',p.mp_identite'
      ',p.mp_position'
      'FROM t_union u '
      'inner join evenements_fam f on f.ev_fam_kle_famille=u.union_clef'
      'inner join sources_record s on s.type_table=''F'' and s.data_id=f.ev_fam_clef'
      'inner join MEDIA_POINTEURS p on p.mp_type_image=''F'' and p.mp_pointe_sur=s.id'
      '  and p.mp_cle_individu=:indi'
      'inner join MULTIMEDIA m on p.MP_MEDIA=m.MULTI_CLEF'
      'WHERE u.union_mari=:indi or u.union_femme=:indi'
      'order by 1 nulls last,9'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'indi'
        ParamType = ptInput
      end>
    UpdateObject = IBUMultimedia
    left = 24
    top = 224
