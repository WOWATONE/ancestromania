      ',e.ev_fam_acte'
      ',e.ev_fam_insee'
      ',e.ev_fam_ordre'
      ',e.ev_fam_heure'
      ',case e.ev_fam_type'
      '    when ''EVEN'' then iif(char_length(e.ev_fam_titre_event)'
      '>0,e.ev_fam_titre_event,r.ref_eve_lib_long)'
      '    else r.ref_eve_lib_long'
      '    end as ev_fam_titre_event'
      ',e.id_import_gedcom'
      ',e.ev_fam_date_mois'
      ',e.ev_fam_date_mois_fin'
      ',e.ev_fam_date_year_fin'
      ',e.ev_fam_latitude'
      ',e.ev_fam_longitude'
      ',e.ev_fam_description'
      ',e.ev_fam_cause'
      ',e.ev_fam_date_jour'
      ',e.ev_fam_date_jour_fin'
      ',e.ev_fam_type_token1'
      ',e.ev_fam_type_token2'
      ',e.ev_fam_datecode'
      ',e.ev_fam_datecode_tot'
      ',e.ev_fam_datecode_tard'
      ',e.ev_fam_calendrier1'
      ',e.ev_fam_calendrier2'
      ',r.REF_EVE_CAT'
      ',(select 1 from rdb$database'
      '  where exists (select * from media_pointeurs p where '
      'p.mp_pointe_sur=e.ev_fam_clef'
      '                             and p.mp_table=''F'''
      '                             and p.mp_type_image=''A'')) as MULTI_CLEF'
      ',(select 1 from rdb$database'
      '  where exists (select *'
      '                      from sources_record s'
      '                      left join media_pointeurs mp on mp.mp_table=''F'''
      '                                                 and mp.mp_type_image=''F'''
      '                                                 and mp.mp_pointe_sur=s.id'
      '                      where s.type_table=''F'''
      '                        and s.data_id=e.ev_fam_clef'
      '                        and (char_length(s.auth)>0'
      '                             or char_length(s.titl)>0'
      '                             or char_length(s.abr)>0'
      '                             or char_length(s.publ)>0'
      '                             or char_length(s.texte)>0'
      '                             or mp.mp_media is not null)))  as '
      '"EV_FAM_DETAILS" '
      'from EVENEMENTS_FAM e'
      'inner join ref_evenements r on '
      'r.ref_eve_lib_court=e.ev_fam_type and '
      'r.ref_eve_langue=:langue'
      'WHERE e.EV_FAM_KLE_FAMILLE =:I_UNION'
      'ORDER BY e.ev_fam_ordre nulls last'
      ',e.EV_FAM_DATECODE nulls last'
      ',e.ev_fam_heure nulls last'
      ',e.ev_fam_clef'
