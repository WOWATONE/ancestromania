  object IBConjoints: TIBSQL[16]
    Database = dm.ibd_BASE
    ParamCheck = True
    SQL.Strings = (
      'select pr.conjoint'
      ',c.nom'
      ',c.prenom'
      ',c.sexe'
      ',c.annee_naissance'
      ',c.date_naissance'
      ',c.annee_deces'
      ',c.num_sosa'
      ',c.cle_pere'
      ',c.cle_mere'
      ',pr.clef_union'
      ',p.nom as nom_pere'
      ',p.prenom as prenom_pere'
      ',p.annee_naissance as pere_naissance'
      ',p.annee_deces as pere_deces'
      ',m.nom as nom_mere'
      ',m.prenom as prenom_mere'
      ',m.annee_naissance as mere_naissance'
      ',m.annee_deces as mere_deces'
      ',(select first(1) ev_fam_date_year from evenements_fam'
      '  where ev_fam_kle_famille=pr.clef_union and ev_fam_type=''MARR'' and ev_fam_date_year is not null'
      '  order by ev_fam_datecode) as an_mar'
      ',(select first(1) ev_fam_date_writen from evenements_fam'
      '  where ev_fam_kle_famille=pr.clef_union and ev_fam_type=''MARR'' and ev_fam_date_year is not null'
      '  order by ev_fam_datecode) as date_mar'
      'from proc_conjoints_ordonnes(:clef_fiche,0) pr'
      'inner join individu c on c.cle_fiche=pr.conjoint'
      'left join individu p on p.cle_fiche=c.cle_pere'
      'left join individu m on m.cle_fiche=c.cle_mere'
      'order by pr.ordre'
    )
    Transaction = dm.IBT_BASE
    left = 192
    top = 72
  end
